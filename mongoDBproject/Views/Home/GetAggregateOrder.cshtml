@model IEnumerable<mongoDBproject.Models.Order>

@{
    ViewData["Title"] = "GetAggregateOrder";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>GetAggregateOrder</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateOrder)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CountOrder)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateOrder)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CountOrder)
            </td>
        </tr>
}
    </tbody>
</table>
<a asp-action="GetAggregateOrders">Back to Index</a>